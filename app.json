{
  "expo": {
    "name": "NIYAH",
    "slug": "niyah",
    "owner": "niyah-app",
    "version": "1.0.0",
    "scheme": "niyah",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1714"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.niyah.app",
      "usesAppleSignIn": true,
      "associatedDomains": ["applinks:niyah-b972d.firebaseapp.com"],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1A1714"
      },
      "package": "com.niyah.app",
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "niyah-b972d.firebaseapp.com",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.714586513020-42pm28b387sklqgl2fp3dsm16cht01rk"
        }
      ],
      "expo-apple-authentication",
      "./plugins/withFollyCoroutinesFix",
      "./plugins/withGoogleServicesPlist",
      "./plugins/withFirebaseStaticFrameworks"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "nativeModulesDir": "modules",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "70318998-fbd4-4d83-939f-39190c8cfdb4"
      }
    }
  }
}
