{
  "name": "niyah",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "packageManager": "pnpm@10.29.3",
  "scripts": {
    "start": "expo start --dev-client",
    "start:go": "expo start --go",
    "android": "expo start --android --dev-client",
    "ios": "expo start --ios --dev-client",
    "web": "expo start --web",
    "build:dev": "eas build --profile development --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "build:dev:device": "eas build --profile development-device --platform ios",
    "build:preview": "eas build --profile preview --platform all",
    "build:production": "eas build --profile production --platform all",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --testPathPattern=unit",
    "test:integration": "vitest run --testPathPattern=integration",
    "test:stores": "vitest run --testPathPattern=store",
    "test:components": "vitest run --testPathPattern=components",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"app/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"app/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "ci": "pnpm lint && pnpm typecheck && pnpm test"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-google-signin/google-signin": "^14.0.2",
    "eas": "^0.1.0",
    "eas-cli": "^18.0.1",
    "expo": "~54.0.33",
    "expo-apple-authentication": "^8.0.8",
    "expo-auth-session": "^7.0.10",
    "expo-blur": "^15.0.8",
    "expo-build-properties": "^1.0.10",
    "expo-constants": "~18.0.13",
    "expo-crypto": "^15.0.8",
    "expo-dev-client": "^6.0.20",
    "expo-haptics": "^15.0.8",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "^1.0.8",
    "expo-web-browser": "^15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.15.3",
    "react-native-worklets": "^0.7.3",
    "sf-symbols-typescript": "^2.2.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/react-native": "^12.9.0",
    "@types/react": "~19.1.17",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "babel-preset-expo": "^54.0.10",
    "eslint": "^9.39.2",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^25.0.1",
    "prettier": "^3.8.1",
    "react-native-web": "~0.20.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^2.1.9"
  },
  "private": true
}
